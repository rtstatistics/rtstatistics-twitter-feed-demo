<?xml version="1.0" encoding="UTF-8"?>
<configuration>

	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{yyyy-MM-dd HH:mm:ss.SSS ZZZ} %-25.-25thread %-5level %-56logger{56} %msg%n</pattern>
		</encoder>
	</appender>

	<logger name="com.rtstatistics.demo" level="debug"/>

	<root level="info">
		<appender-ref ref="STDOUT" />
	</root>

</configuration>